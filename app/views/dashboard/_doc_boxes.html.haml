.col-xs-12.doc-box
  .col-xs-12
    %table.table.table-striped
      - @documents.each do |d|
        %tr
          %td
            %ul.list-unstyled
              %li.date= d.document.created_at.strftime("%b %d, %Y")
              %li
                = link_to doc_title(d.document), document_path(d.document)

